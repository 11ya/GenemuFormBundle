<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="genemu_recaptcha.form.type.class">Genemu\Bundle\FormBundle\Form\Type\ReCaptchaType</parameter>
        <parameter key="genemu_tinymce.form.type.class">Genemu\Bundle\FormBundle\Form\Type\TinymceType</parameter>
        <parameter key="genemu_doublelist.form.type.class">Genemu\Bundle\FormBundle\Form\Type\DoubleListType</parameter>
        <parameter key="genemu_jquerydate.form.type.class">Genemu\Bundle\FormBundle\Form\Type\JQueryDateType</parameter>
        <parameter key="genemu_jqueryautocompleter.form.type.class">Genemu\Bundle\FormBundle\Form\Type\JQueryAutocompleterType</parameter>
    </parameters>

    <services>
        <service id="genemu_recaptcha.form.type" class="%genemu_recaptcha.form.type.class%">
            <tag name="form.type" alias="genemu_recaptcha" />
            <argument type="service" id="request" strict="false" />
            <argument>%genemu.form.recaptcha.theme%</argument>
            <argument>%genemu.form.recaptcha.public_key%</argument>
            <argument>%genemu.form.recaptcha.use_ssl%</argument>
            <argument>%genemu.form.recaptcha.server_url%</argument>
            <argument>%genemu.form.recaptcha.server_url_ssl%</argument>
        </service>
        <service id="genemu_tinymce.form.type" class="%genemu_tinymce.form.type.class%">
            <tag name="form.type" alias="genemu_tinymce" />
            <argument>%genemu.form.tinymce.theme%</argument>
            <argument>%genemu.form.tinymce.script_url%</argument>
            <argument>%genemu.form.tinymce.width%</argument>
            <argument>%genemu.form.tinymce.height%</argument>
            <argument>%genemu.form.tinymce.config%</argument>
        </service>
        <service id="genemu_doublelist.form.type" class="%genemu_doublelist.form.type.class%">
            <tag name="form.type" alias="genemu_doublelist" />
            <argument>%genemu.form.doublelist.class%</argument>
            <argument>%genemu.form.doublelist.class_select%</argument>
            <argument>%genemu.form.doublelist.label_associated%</argument>
            <argument>%genemu.form.doublelist.label_unassociated%</argument>
            <argument>%genemu.form.doublelist.associated_first%</argument>
        </service>
        <service id="genemu_jquerydate.form.type" class="%genemu_jquerydate.form.type.class%">
            <tag name="form.type" alias="genemu_jquerydate" />
            <argument>%genemu.form.jquerydate.image%</argument>
            <argument>%genemu.form.jquerydate.config%</argument>
        </service>
        <service id="genemu_jqueryautocompleter.form.type" class="%genemu_jqueryautocompleter.form.type.class%">
            <tag name="form.type" alias="genemu_jqueryautocompleter" />
            <argument>%genemu.form.jqueryautocompleter.url%</argument>
            <argument>%genemu.form.jqueryautocompleter.value_callback%</argument>
            <argument>%genemu.form.jqueryautocompleter.config%</argument>
        </service>
    </services>
</container>