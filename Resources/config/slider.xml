    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="genemu.form.type.slider" class="%genemu.form.namespace.type%\SliderType">
            <tag name="form.type" alias="genemu_slider" />
        </service>
        <service id="genemu.form.type.jqueryslider" class="%genemu.form.namespace.jquery%\SliderType">
            <tag name="form.type" alias="genemu_jqueryslider" />
        </service>
    </services>

</container>
