<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="genemu.form.type.jqueryautocompleter.class">Genemu\Bundle\FormBundle\Form\Type\JQuery\AutocompleterType</parameter>
        <parameter key="genemu.form.type.ajaxentity.class">Genemu\Bundle\FormBundle\Form\Type\Entity\AjaxType</parameter>
        <parameter key="genemu.form.type.ajaxdocument.class">Genemu\Bundle\FormBundle\Form\Type\Document\AjaxDocumentType</parameter>
        <parameter key="genemu.form.type.ajaxmodel.class">Genemu\Bundle\FormBundle\Form\Type\Model\AjaxModelType</parameter>
    </parameters>

    <services>
        <service id="genemu.form.type.jqueryautocompleter" class="%genemu.form.type.jqueryautocompleter.class%">
            <tag name="form.type" alias="genemu_jqueryautocompleter" />
        </service>
        <service id="genemu.form.type.ajaxentity" class="%genemu.form.type.ajaxentity.class%">
            <tag name="form.type" alias="genemu_ajaxentity" />
            <argument type="service" id="doctrine" />
        </service>
        <service id="genemu.form.type.ajaxdocument" class="%genemu.form.type.ajaxdocument.class%">
            <tag name="form.type" alias="genemu_ajaxdocument" />
            <argument type="service" id="doctrine.odm.mongodb" />
        </service>
        <service id="genemu.form.type.ajaxmodel" class="%genemu.form.type.ajaxmodel.class%">
            <tag name="form.type" alias="genemu_ajaxmodel" />
        </service>
    </services>

</container>
