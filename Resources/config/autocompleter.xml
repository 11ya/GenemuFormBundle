<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="genemu.form.type.autocompleter" class="%genemu.form.namespace.type%\AutocompleterType">
            <tag name="form.type" alias="genemu_autocompleter" />
        </service>
        <service id="genemu.form.type.jqueryautocompleter" class="%genemu.form.namespace.jquery%\AutocompleterType">
            <tag name="form.type" alias="genemu_jqueryautocompleter" />
        </service>
        <service id="genemu.form.type.jquerychosen" class="%genemu.form.namespace.jquery%\ChosenType">
            <tag name="form.type" alias="genemu_jquerychosen" />
        </service>

        <service id="genemu.form.type.ajaxmodel" class="%genemu.form.namespace.model%\AjaxModelType">
            <tag name="form.type" alias="genemu_ajaxmodel" />
        </service>
    </services>

</container>
